{
    "configurations": [
        {
            "name": "STM32F2/4",
            "includePath": [
                "${workspaceFolder}/hw/**",
                "${workspaceFolder}/os/**"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/arm-none-eabi-g++",
            "cStandard": "gnu11",
            "cppStandard": "c++20",
            "intelliSenseMode": "linux-gcc-arm",
            "compilerArgs": [
                "-Wall",
                "-Wextra",
                "-Weffc++",
                "-fno-exceptions",
                "-fno-unwind-tables",
                "-fno-rtti"
            ],
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}