{
    "configurations": [
        {
            "name": "Attach to Running QEMU",
            "type": "gdb",
            "request": "attach",
            "executable": "${workspaceRoot}/build/startup.elf",
            "gdbpath": "/usr/bin/gdb-multiarch",
            "target": "localhost:63770",
